{
  "name": "watermark-lite",
  "type": "module",
  "version": "0.0.6",
  "packageManager": "pnpm@9.10.0",
  "description": "💻 a lightweight watermark plugin 一个轻量级水印插件",
  "author": "xiaohe0601 <xiaohe0601@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/xiaohe0601",
  "homepage": "https://github.com/xiaohe0601/watermark-lite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaohe0601/watermark-lite.git"
  },
  "bugs": "https://github.com/xiaohe0601/watermark-lite/issues",
  "keywords": [
    "watermark"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "play": "nr -C playground dev",
    "build": "unbuild",
    "prepublishOnly": "nr build",
    "release": "bumpp package.json playground/package.json",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "lint:type": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.2",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^20.16.5",
    "bumpp": "^9.5.2",
    "eslint": "^9.10.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.6",
    "vitest": "^2.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}